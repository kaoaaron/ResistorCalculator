    $(".dropdown dt").not(this).next().find("ul").hide();
    $(this).next().find("ul").toggle();
});
var firstband, secondband, multiplier, tolerance, toleranceSearch;
$(".dropdown dd ul li").click(function() {
    whichDrop = $(this).closest("dl").prop("id");
    if (whichDrop == "firstband") {
        firstband = $(this).attr("data-firstband-value");
    } else if (whichDrop == "secondband") {
        secondband = $(this).attr("data-secondband-value");
    } else if (whichDrop == "multiplier") {
        multiplier = $(this).attr("data-multiplier-value");
    } else if (whichDrop == "tolerance") {
        tolerance = $(this).attr("data-tolerance-value");
        toleranceSearch = $(this).attr("data-tolerance-search");
    }
    $(this).closest("dl").find("dt").html($(this).html());
    $(this).parent().hide();
    if (firstband !== undefined && secondband !== undefined && multiplier !== undefined && tolerance !== undefined) {
        calculate();
    }

    function calculate() {
        var mult, resDisplay, resSearch, res;

        res = ((100 * firstband) + (10 * secondband)) * multiplier / 1000;

        if (res >= 1e9) {
            res /= 1e9;
            mult = "G";
        } else if (res >= 1e6) {
            res /= 1e6;
            mult = "M";
        } else if (res >= 1e3) {
            res /= 1e3;
            mult = "k";
        } else {
            mult = " ";
        }

        resDisplay = res + mult + " Ohms";
        resSearch = res + "+" + mult + "Ohms";

        if (tolerance != "") {
            resDisplay += " " + tolerance;
        }

        var resistor0Search = "/products/" + __headerData.lang + "/resistors/through-hole-resistors/53";
        var psAjaxUrl = "/categoryproductlist/" + __headerData.lang + "/53/6?pv3=" + toleranceSearch + "&pv2085=u" + resSearch;
        var psUrl = "/products/" + __headerData.lang + "/resistors/through-hole-resistors/53?pv3=" + toleranceSearch + "&pv2085=u" + resSearch;
        var keywordUrl = "/products/" + __headerData.lang + "?keywords=" + resDisplay;
        $("#custom-search").text(resDisplay);


        var RelResT, ViewResultsT, NoMatchT, KeywordSearchT;
        if ((__headerData.lang == "zh" && __headerData.site == "CN") || (__headerData.lang == "zh" && __headerData.site == "SG")) {
            RelResT = "相关电阻器：", ViewResultsT = "查看全部结果", NoMatchT = "无匹配结果 - ", KeywordSearchT = "关键词搜索";
        } else if ((__headerData.lang == "zh" && __headerData.site == "HK") || (__headerData.lang == "zh" && __headerData.site == "TW")) {
            RelResT = "相關電阻：", ViewResultsT = "查看全部結果", NoMatchT = "無符合結果 - ", KeywordSearchT = "關鍵字搜尋";
        } else if (__headerData.lang == "fr") {
            RelResT = "Résistances associées :", ViewResultsT = "Afficher tous les résultats", NoMatchT = "Aucune correspondance - ", KeywordSearchT = "Recherche par mot-clé";
        } else if (__headerData.lang == "de") {
            RelResT = "Verwandte Widerstände:", ViewResultsT = "Alle Ergebnisse anzeigen", NoMatchT = "Keine Treffer - ", KeywordSearchT = "Stichwortsuche";
        } else if (__headerData.lang == "he") {
            RelResT = "נגדים קשורים:", ViewResultsT = "הצגת כל התוצאות", NoMatchT = "אין התאמות - ", KeywordSearchT = "חיפוש לפי מילות חיפוש";
        } else if (__headerData.lang == "it") {
            RelResT = "Resistori correlati:", ViewResultsT = "Visualizza tutti i risultati", NoMatchT = "Nessuna corrispondenza - ", KeywordSearchT = "Ricerca per parole chiave";
        } else if (__headerData.lang == "ja") {
            RelResT = "関連抵抗器：", ViewResultsT = "全ての結果を閲覧：", NoMatchT = "一致なし - ", KeywordSearchT = "キーワード検索";
        } else if (__headerData.lang == "ko") {
            RelResT = "관련 저항기:", ViewResultsT = "결과 모두 보기", NoMatchT = "검색 결과 없음 - ", KeywordSearchT = "키워드 검색";
        } else if (__headerData.lang == "es") {
            RelResT = "Resistencias relacionadas:", ViewResultsT = "Ver todos los resultados", NoMatchT = "No hay coincidencias. ", KeywordSearchT = "Búsqueda con palabra clave";
        } else {
            RelResT = "Related Resistors:", ViewResultsT = "View All Results", NoMatchT = "No Matches - ", KeywordSearchT = "Keyword Search";
        }

		        $.ajax({
            type: "GET",
            url: psAjaxUrl,
            dataType: 'json',
            data: {
                get_param: 'value'
            },
            success: function(result) {
                $(".calcResults, .calcViewResults, .resCalcSearch, .relatedResistors").html("");
                var output = "";

                if (result.TotalParts == 0) {
                    $("<span>" + NoMatchT + "<a href='" + keywordUrl + "'>" + KeywordSearchT + "</a></span>").appendTo(".calcViewResults");
                    $("<a href='" + keywordUrl + "'><span class='icon-search'></span></a>").appendTo(".resCalcSearch");
                    $("<h2 class='headline-border'>" + RelResT + "</h2>").appendTo(".relatedResistors");
                } else {
                    for (var i in result.PartList) {
                        console.log(result.PartList[i].PartNumber)
                        output += "<div class='calcResult'>" + "<a href='" + result.PartList[i].ProductLink + "'>" + "<img src='" + result.PartList[i].Image + "' />" + "<br>" + result.PartList[i].PartNumber + "</a>" +
                            "<span>" + "<a href='" + result.PartList[i].SmcLink + "'>" + result.PartList[i].Manufacturer + "</a>" + "</span>" +
                            result.PartList[i].Description + "<br>" + result.PartList[i].UnitPrice + "</div>";
                    }
                }
                if (result.TotalParts > 6) {
                    $("<a href='" + psUrl + "'><button class='button primary'>" + ViewResultsT + "</button></a>").appendTo(".calcViewResults");
                    $("<a href='" + psUrl + "'><span class='icon-search'></span></a>").appendTo(".resCalcSearch");
                    $("<h2 class='headline-border'>" + RelResT + "</h2>").appendTo(".relatedResistors");
                }

                if ((result.TotalParts > 0) && (result.TotalParts <= 6)) {
                    $("<a href='" + keywordUrl + "'><span class='icon-search'></span></a>").appendTo(".resCalcSearch");
                    $("<h2 class='headline-border'>" + RelResT + "</h2>").appendTo(".relatedResistors");
                }
                output += "";
                $(".calcResults").html(output);
            }
        });
    }
});

document.addEventListener("click", function(e) {
    if (!__helpers.hasParentWithClass(__helpers.getTarget(e), 'dropdown') && !__helpers.hasParentWithClass(__helpers.getTarget(e), 'header__resource')) {
        $(".dropdown ul").hide();
    }
});

$(".dropdown img.color").addClass("colorvisibility");
